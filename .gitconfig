[user]
	name = meteorsoftware@qq.com
	email = meteorsoftware@qq.com
[branch]
    autosetuprebase = always
[push]
    default = tracking
[core]
    editor = vim
[color]
    status = auto
    branch = auto
    diff = auto
    ui = true
    pager = true
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
